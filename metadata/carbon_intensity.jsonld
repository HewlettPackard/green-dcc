    "@type": "sc:Dataset",
    "name": "Carbon Intensity Dataset",
    "description": "A dataset containing carbon intensity measurements for various global regions. The data includes direct and lifecycle carbon intensity measurements in gCO₂eq/kWh, as well as percentages of low carbon and renewable energy sources for different geographical zones.",
    "conformsTo": "http://mlcommons.org/croissant/1.0",
    "license": "https://opensource.org/licenses/MIT",
    "url": "https://github.com/HewlettPackard/green-dcc/tree/main",
    "version": "1.0.0",
    "citeAs": "Hewlett Packard Enterprise (2023). Carbon Intensity Dataset. GitHub. https://github.com/HewlettPackard/green-dcc/tree/main",
    "creator": {
        "@type": "sc:Organization",
        "name": "Hewlett Packard Enterprise",
        "url": "https://www.hpe.com"
    },
    "isLiveDataset": true,
    "datePublished": "2023",
    "keywords": [
        "carbon intensity",
        "renewable energy",
        "climate data",
        "electricity",
        "emissions",
        "sustainability"
    ],
