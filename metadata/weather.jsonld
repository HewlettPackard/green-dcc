    "license": "https://opensource.org/licenses/MIT",
    "url": "https://github.com/HewlettPackard/green-dcc/tree/main",
    "version": "1.0.0",
    "citeAs": "Hewlett Packard Enterprise (2023). Weather Dataset. GitHub. https://github.com/HewlettPackard/green-dcc/tree/main",
    "creator": {
        "@type": "sc:Organization",
        "name": "Hewlett Packard Enterprise",
        "url": "https://www.hpe.com"
    },
    "isLiveDataset": true,
    "datePublished": "2023",
    "keywords": [
